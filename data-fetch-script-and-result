  var quotes = document.getElementsByClassName('bqQuoteLink')
  var quoters = document.getElementsByClassName('bq-aut')
  var sources = [
                  "https://www.brainyquote.com/quotes/keywords/open_source.html",
                  "https://www.brainyquote.com/quotes/keywords/open_source_2.html",
                  "https://www.brainyquote.com/quotes/keywords/open_source_3.html"
                ];
sources.forEach(function(source){
if(document.location.toString() === source){
  for(var i = 0; i < quotes.length; i++) {
    var  quote = quotes[i].innerHTML.split('>')[1].split('<')[0];
    var quoter = quoters[i].innerHTML.split('>')[1].split('<')[0];
    console.log('{ "quote": "'+quote+'", "quoter" : "'+quoter+'", "source": "'+source+'" },');
  }
}
});
